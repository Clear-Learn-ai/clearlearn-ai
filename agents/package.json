{
  "name": "ai-tutor-agents",
  "version": "1.0.0",
  "description": "Multi-agent system for AI tutor focusing on pre-med organic chemistry",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "debug": "tsx --inspect-brk src/index.ts",
    "agent:content": "tsx src/agents/content-specialist/standalone.ts",
    "agent:pedagogy": "tsx src/agents/pedagogy/standalone.ts",
    "agent:visual": "tsx src/agents/visual-learning/standalone.ts",
    "agent:assessment": "tsx src/agents/assessment/standalone.ts",
    "agent:conversation": "tsx src/agents/conversation/standalone.ts",
    "agent:resource": "tsx src/agents/resource/standalone.ts",
    "orchestrator": "tsx src/orchestrator/standalone.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "ws": "^8.17.0",
    "node-fetch": "^3.3.2",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.12",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "rimraf": "^5.0.7"
  },
  "keywords": [
    "ai",
    "tutor",
    "multi-agent",
    "organic-chemistry",
    "education",
    "pre-med",
    "mcp",
    "claude",
    "openai"
  ],
  "author": "AI Tutor Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}